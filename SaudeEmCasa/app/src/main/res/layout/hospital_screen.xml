<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context=".view.HospitalScreen"
android:background="#F5F5F5"
android:contextClickable="false"
    android:id="@+id/hospital_map">

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:text="hospitalName"
    android:id="@+id/textViewHospName"
    android:layout_centerVertical="true"
    android:layout_centerHorizontal="true" />

<TextView

    android:layout_width="200dip"
    android:layout_height="80dip"
    android:textAppearance="?android:attr/textAppearanceSmall"
    android:text="Address"
    android:id="@+id/textViewAddressHosp"
    android:layout_gravity="center_horizontal"
    android:layout_marginTop="63dp"
    android:layout_below="@+id/textViewHospName"
    android:layout_alignParentStart="true" />


<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="Telephone"
    android:id="@+id/textViewHospTel"
    android:layout_below="@+id/textViewAddressHosp"
    android:layout_alignStart="@+id/textViewAddressHosp"
    android:layout_marginTop="33dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/textViewHospName">

        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="mds.gpp.saudeemcasa.view.MapToHospital"
            android:id="@+id/hospitaMapFragment"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true" />
    </RelativeLayout>

    <!-- android:layout_alignParentStart="true" /> -->

</RelativeLayout>